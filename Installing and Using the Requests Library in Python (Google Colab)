        "import requests\n",
        "url= \"https://api.open-meteo.com/v1/forecast\"\n",
        "\n",
        "#Here we will setup the request parameters\n",
        "\n",
        "params= {\n",
        "    \"latitude\":53.3331,\n",
        "    \"longitude\": -6.248,\n",
        "    \"hourly\": \"temperature_2m,cloud_cover_mid,wind_direction_120m,soil_moisture_1_to_3cm\",\n",
        "    \"timezone\":\"Europe/London\",\n",
        "    \"past_days\":5\n",
        "}\n",
        "\n",
        "#Now we will ask the website for the data using the url based on the parameters\n",
        "\n",
        "response=requests.get(url, params=params)\n",
        "\n",
        "# if we get this successfully we will get the data from the website\n",
        "\n",
        "if response.status_code==200:\n",
        "    data=response.json()\n",
        "    print(data)\n",
        "else:\n",
        "    print(\"Error:\",response.status_code,response.text)\n",
